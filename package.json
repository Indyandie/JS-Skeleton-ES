{
  "name": "js-skeleton-es",
  "version": "1.0.0",
  "description": "A JS-Skeleton-ES Project for Javascript Libraries",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=12.17.0"
  },
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "make-clean": "node -e \"fs.readdirSync('o').forEach(function(file){ if(file=='.stub')return ;fs.unlinkSync('o/'+file)});\"",
    "make-build-cjs": "rollup index.js --format cjs  --file o/index.cjs",
    "make-build-mjs": "rollup index.js --format es   --file o/index.mjs",
    "make-build-iife": "rollup index.js --format iife --file o/index.iife.js --name JS_Skeleton_ES",
    "make-build-all": "npm run-script test && { npm run-script make-clean;npm run-script make-build-cjs; npm run-script make-build-mjs; npm run-script make-build-iife; }",
    "make-minify-cjs": "./node_modules/uglify-es/bin/uglifyjs o/index.cjs     > o/index.min.cjs",
    "make-minify-mjs": "./node_modules/uglify-es/bin/uglifyjs o/index.mjs     > o/index.min.mjs",
    "make-minify-iife": "./node_modules/uglify-es/bin/uglifyjs o/index.iife.js > o/index.iife.js",
    "make-minify-all": "npm run-script make-minify-cjs ; npm run-script make-minify-mjs; npm run-script make-minify-iife",
    "make-readme": "./node_modules/.bin/jsdoc2md --template README.hb --files index.js Classes/*.js Functions/*.js > README.md",
    "test": "./node_modules/.bin/mocha Tests Tests/Functions Tests/Classes",
    "test-coverage": "./node_modules/c8/bin/c8.js ./node_modules/.bin/mocha Tests Tests/Functions Tests/Classes"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/node": "^14.0.6",
    "@types/sinon": "^9.0.4",
    "c8": "^7.1.2",
    "expect.js": "^0.3.1",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^7.2.0",
    "rollup": "^2.11.2",
    "sinon": "^9.0.2",
    "uglify-es": "^3.3.9"
  },
  "author": "James V. Andino"
}
